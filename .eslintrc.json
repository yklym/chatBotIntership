{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": 0,
        "arrow-parens": 0,
        "new-cap": 0,
        "quotes" : 0,
        "func-names": 0,
        "no-underscore-dangle": 0,
        "indent": 0
      }
}